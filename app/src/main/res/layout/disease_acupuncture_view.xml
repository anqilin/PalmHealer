<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/common_top_bar"></include>

    <ImageView
        android:id="@+id/common_disease_bt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="342px"
        android:background="@mipmap/button_disease"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <TextView
        android:layout_marginTop="642px"
        android:gravity="center"
        android:text="常见病"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/acupuncture_bt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="926px"
        android:background="@mipmap/button_acup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <TextView
        android:layout_marginTop="1226px"
        android:gravity="center"
        android:text="穴位功效"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</com.zhy.autolayout.AutoRelativeLayout>